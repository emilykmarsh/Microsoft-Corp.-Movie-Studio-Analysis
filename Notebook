    "ax.set_title('Total Box Office and Movies Produced Per Year', fontsize= 20)\n",
    "ax.yaxis.set_major_formatter(mtick.StrMethodFormatter('${x:,.0f}'))\n",
    "\n",
    "plt.xticks(ticks= resultFinal['production_year'], labels = resultFinal['production_year'])\n",
    "\n",
    "fig.legend(loc='upper right', bbox_to_anchor=(.88,.88))\n",
    "\n",
    "plt.savefig(\"SummaryChart.jpg\");\n"
   ]

    "# Grouping the dataset by movie rating and calculating the average profit per movie to account for some movie ratings having more movies in the dataset then others\n",
    "\n",
    "opusTopRating = (opusClean\n",
    "                .groupby(['rating'])\n",
    "                .mean()\n",
    "                .reset_index()\n",
    "                .sort_values(by='total_profit', \n",
    "                        ascending=False)\n",
    "                .head()\n",
    "                               )\n",

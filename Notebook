    "# Creating a stacked bar chart to show total box office profits for each year and how much of that profit was domestic and international\n",
    "\n",
    "fig,ax  = plt.subplots(figsize=(14,7))\n",
    "\n",
    "ax1 = plt.subplot()\n",
    "ax2 = ax1.twinx()\n",
    "\n",
    "ax1.bar(x=resultFinal['production_year'], height=resultFinal['international_box_office'], color=(0.2, 0.4, 0.6, 0.6), label = 'International Box Office' )\n",
    "ax1.bar(x=resultFinal['production_year'], height=resultFinal['domestic_box_office'], label = 'Domestic Box Office')\n",
    "\n",
    "ax2.plot(resultFinal['production_year'], resultFinal['movie_odid'], color='orange', label='Total Movies Produced')\n",
    "\n",
    "ax.set_xlabel('Year', fontsize= 18)\n",
    "ax1.set_ylabel('Profit in Dollars', fontsize= 18)\n",

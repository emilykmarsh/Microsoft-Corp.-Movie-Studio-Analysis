    "# Creating a scatter plot to show the correlation between budget and profit\n",
    "\n",
    "fig,ax  = plt.subplots(figsize=(14,7))\n",
    "sns.regplot(x=opusClean[\"production_budget\"], y=opusClean[\"total_profit\"])\n",
    "\n",
    "ax.set_xlabel('Producution Budget', fontsize=18)\n",

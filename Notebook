   "source": [
    "# Grouping the dataset by movie genre and calculating the average profit per movie to account for some genres having more movies in the dataset then others\n",
    "\n",
    "opusTopGenre = (opusClean\n",
    "                .groupby(['genre'])\n",
    "                .mean()\n",
    "                .reset_index()\n",
    "                .sort_values(by='total_profit', \n",
    "                    ascending=False)\n",
    "                .head()\n",
    "                           )\n",
    "opusTopGenre"
   ]
  },

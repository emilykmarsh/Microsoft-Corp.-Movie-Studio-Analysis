   "source": [
    "# Grouping the dataset by source material and calculating the average profit per movie to account for some source materials having more movies in the dataset then others\n",
    "\n",
    "opusTopSource = (opusClean\n",
    "                .groupby(['source'])\n",
    "                .mean()\n",
    "                .reset_index()\n",
    "                .sort_values(by='total_profit', \n",
    "                    ascending=False)\n",
    "                .head()\n",
    "                           )\n",
    "opusTopSource\n"
   ]
  },
